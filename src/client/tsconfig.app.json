{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "module": "es2015",
    "baseUrl": "",
    "types": [],
    "paths": {
      "@node_modules/*": ["../../node_modules/*"],
      "@shared/*": ["app/modules/shared/*"],
      "@utils/*": ["app/utils/*"],
      "@environment/*": ["environment/*"],
      "@guards/*": ["app/guards/*"],
      "@models/*": ["app/models/*"],
      "@modules/*": ["app/modules/*"],
      "@resolvers/*": ["app/resolvers/*"],
      "@services/*": ["app/services/*"],
      "@pipes/*": ["app/pipes/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
